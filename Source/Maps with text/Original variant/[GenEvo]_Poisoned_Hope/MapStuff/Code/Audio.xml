		MinVolume = "105"
		Priority = "HIGH"
		Type = "WORLD PLAYER VOICE"
		SubmixSlider = "VOICE" >
		<PitchShift Low="-1" High="1" />
		<Sound>vshiata</Sound> 
		<Sound>vshiatb</Sound> 
		<Sound>vshiatc</Sound> 
		<Sound>vshiatd</Sound>
		<Sound>vshiate</Sound>
	</AudioEvent>
